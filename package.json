{
  "name": "centaur",
  "version": "1.0.0",
  "main": "index.js",
  "license": "MIT",
  "private": true,
  "engines": {
    "node": ">=8.6.0",
    "yarn": ">=1.1.0"
  },
  "scripts": {
    "lint": "stylint --reporter stylint-stylish src && eslint . --ext .js,.jsx --fix",
    "test": "NODE_ENV=test jest --coverage --maxWorkers",
    "start": "gulp start",
    "build": "NODE_ENV=production gulp build",
    "precommit": "NODE_ENV=test ./lib/scripts/pre-commit.sh"
  },
  "precommit": [
    "precommit"
  ],
  "browserslist": [
    ">1%",
    "last 4 versions",
    "Firefox ESR",
    "not ie < 9"
  ],
  "babel": {
    "presets": [
      [
        "env",
        {
          "targets": {
            "node": "current"
          }
        }
      ],
      "react"
    ],
    "plugins": [
      "transform-decorators-legacy",
      "transform-class-properties",
      "transform-object-rest-spread"
    ]
  },
  "jest": {
    "rootDir": "./src",
    "bail": false,
    "verbose": false,
    "timers": "fake",
    "cacheDirectory": "<rootDir>/../tmp/caches/jest",
    "coverageDirectory": "<rootDir>/../tmp/coverage",
    "moduleFileExtensions": [
      "js",
      "jsx",
      "json"
    ],
    "setupTestFrameworkScriptFile": "<rootDir>/../lib/tests/AppTestSetup.js",
    "moduleNameMapper": {
      ".+\\.styl$": "identity-obj-proxy",
      "AppTestUtil": "<rootDir>/../lib/tests/AppTestUtil.js"
    },
    "collectCoverageFrom": [
      "**/app/**/*.{js,jsx}"
    ]
  },
  "dependencies": {
    "axios": "^0.17.0",
    "babel-polyfill": "7.0.0-beta.3",
    "body-parser": "1.18.2",
    "classnames": "2.2.5",
    "clean-css": "4.1.9",
    "cookie-parser": "1.4.3",
    "debug": "^3.1.0",
    "express": "4.16.2",
    "feather-icons": "3.3.0",
    "fluxible": "1.4.0",
    "fluxible-addons-react": "0.2.14",
    "fluxible-router": "1.4.0",
    "helmet": "3.9.0",
    "immutable": "3.8.2",
    "lodash": "4.17.4",
    "pm2": "2.7.2",
    "pretty-error": "2.1.1",
    "prop-types": "15.6.0",
    "react": "16.0.0",
    "react-dom": "16.0.0",
    "react-immutable-proptypes": "2.1.0",
    "react-immutable-pure-component": "1.1.1",
    "react-intl": "2.4.0",
    "serialize-javascript": "1.4.0",
    "serve-favicon": "2.4.5",
    "ua-parser-js": "0.7.17"
  },
  "devDependencies": {
    "assets-webpack-plugin": "3.5.1",
    "babel-core": "7.0.0-beta.3",
    "babel-eslint": "8.0.1",
    "babel-jest": "21.2.0",
    "babel-loader": "7.1.2",
    "babel-node": "7.0.0-beta.3",
    "babel-plugin-transform-class-properties": "7.0.0-beta.3",
    "babel-plugin-transform-decorators-legacy": "1.3.4",
    "babel-plugin-transform-object-rest-spread": "7.0.0-beta.3",
    "babel-plugin-transform-react-constant-elements": "7.0.0-beta.3",
    "babel-plugin-transform-react-inline-elements": "7.0.0-beta.3",
    "babel-plugin-transform-react-remove-prop-types": "0.4.10",
    "babel-preset-env": "7.0.0-beta.3",
    "babel-preset-react": "7.0.0-beta.3",
    "browser-sync": "2.18.13",
    "chokidar": "1.7.0",
    "css-loader": "0.28.7",
    "dotenv": "4.0.0",
    "eslint": "4.10.0",
    "eslint-plugin-import": "2.8.0",
    "eslint-plugin-private-props": "0.3.0",
    "eslint-plugin-react": "7.4.0",
    "gulp": "3.9.1",
    "gulp-load-plugins": "1.5.0",
    "gulp-util": "3.0.8",
    "html-loader": "0.5.1",
    "identity-obj-proxy": "3.0.0",
    "imagemin": "5.3.1",
    "isomorphic-style-loader": "4.0.0",
    "jasmine-expect": "3.8.1",
    "jasmine-reporters": "2.2.1",
    "jest": "21.2.1",
    "json-loader": "0.5.7",
    "jsonlint": "1.6.2",
    "markdown-loader": "2.0.1",
    "mkdirp": "0.5.1",
    "ncp": "2.0.0",
    "nib": "1.1.2",
    "node-mocks-http": "^1.6.6",
    "pre-commit": "1.2.2",
    "react-deep-force-update": "1.1.1",
    "react-dev-utils": "4.2.1",
    "react-error-overlay": "3.0.0",
    "react-fastclick": "3.0.2",
    "react-hot-loader": "3.1.1",
    "react-test-renderer": "16.0.0",
    "rimraf": "2.6.2",
    "run-sequence": "2.2.0",
    "stylint": "1.5.9",
    "stylint-stylish": "1.4.0",
    "stylus": "0.54.5",
    "stylus-loader": "3.0.1",
    "svg-inline-loader": "0.8.0",
    "url-loader": "0.6.2",
    "webpack": "3.8.1",
    "webpack-dev-middleware": "1.12.0",
    "webpack-hot-middleware": "2.20.0",
    "webpack-merge": "4.1.1",
    "webpack-node-externals": "1.6.0"
  }
}
